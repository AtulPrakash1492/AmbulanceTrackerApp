<!DOCTYPE html>
<html lang="en">

<head>
     <!-- ../Partials/MetaTag/ -->
    <%- include('../Partials/Metatag/adminMeta.ejs') %>
    <title>Records</title>
</head>

<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <%- include('../Partials/Sidebar/adminSide.ejs') %>
        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">
                <%- include('../Partials/Navbar/adminNav.ejs') %> 
                 <!-- table of records -->
                <table style="width:100%">
                      <tr>
                        <th>TripId</th>
                        <th>Roll Number</th>
                        <th>Time</th>
                        <th>Distance</th>
                        <th>DriverId</th>

                      </tr>
                      <% for(var i = 0; i < result.length ; i++){ %>
                      <tr>
                        <td><%= result[i].tripid %></td>
                        <td><%= result[i].rollnumber %></td>
                        <td><%= result[i].time %></td>
                        <td><%= result[i].distance %></td>
                        <td><%= result[i].driverid %></td>
                      </tr>
                    <% }; %>
                  </table>
                  <!-- end of table of records -->
            </div>
            <!-- End of Main Content -->
            
        </div>
        <!-- End of Content Wrapper -->

    </div>
    <!-- End of Page Wrapper -->

       <%- include('../Partials/logoutModal.ejs') %>

    <%- include('../Partials/Scripts/adminScripts.ejs') %>
</body>

</html>
